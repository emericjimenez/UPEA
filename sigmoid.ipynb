    "import numpy as np\n",
    "def sigmoid(z): \n",
    "    # calculate the sigmoid of z\n",
    "    r = 1/(1+np.exp(-z))\n",
    "    return r\n",
    "# Test\n",
    "if (sigmoid(0) == 0.5):\n",
    "    print('OK!')\n",
    "else:\n",
    "    print('Nop!')\n",
    "\n",
    "if (sigmoid(5.05) == 0.9936314844931844):\n",
    "    print('RIGHT!')\n",
    "else:\n",
    "    print('Nop again!')"
   ]
  }
